#include <stdio.h>

const int k = 100;

void print_array(int arr[]){
    for(int i=0;i<k;i++){
        printf(arr[i]);
    }
}

void hadamad(int arr1[], int arr2[]){
    int arr3[k]={};
    for(int i=0;i<k;i++){
        arr3[i]=arr1[i]*arr2[i];
    }
    print_array(arr3);
}

int main(){
    int arr1[k] = {};
    int arr2[k] = {};

}